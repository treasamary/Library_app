  <div class="row justify-content-center">
    <div class="col-lg-2"></div>
    <div class="col-lg-4 mymargin">
      <form action="/login" class="login" onsubmit="return validate()">
         <h2>Sign Up</h2> 
         <div class="login-cont">
            <label >Name</label>
            <input type="text" placeholder="enter your Name" style="width:100%;height:2rem;" required>
         </div>  
         <br>
         
         <div class="login-cont">
            <label >Email</label>
            <input type="text" placeholder="enter your emailaddress" id="email" style="width:100%;height:2rem;" required>
         </div>  
            <label for="error" id="error"></label> 
         <br>  
          <div class="login-cont">
             <label >Password</label>
             <input type="password" id="pwd" placeholder="enter your password"  style="width:100%;height: 2rem;" required>
          </div>
          <label for="error4" id="error4"></label> 
          <br>
          <div class="login-cont">
             <label >Confirm Password</label>
             <input type="password" id="pwd2" placeholder="enter your password" style="width:100%;height: 2rem;" required>
         </div>
         <label for="error5" id="error5"></label> 
         <br>
        <div>
            <button id="login-button">Sign Up</button>
        </div>
        <div class="login-misc">
            <h5>Already have an account</h5>
            <a href="/login">Login</a>
        </div>
    </form>
